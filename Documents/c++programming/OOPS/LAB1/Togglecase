

//WAP to convert given string into toggle case.

#include<iostream>
using namespace std;
int main()
{
    char z[100];
    int i;

    cout<<"Enter the string :=";
    cin>>z;

    for(i=0;z[i]!='\0';i++)
    {
        if(z[i]>='a' && z[i]<='z')
        {
            z[i] = z[i] - 32;
        }
        else if(z[i]>='A' && z[i]<='Z')
        {
            z[i] = z[i] + 32;
        }
    }
    cout<<"Given string in toggle case :="<<z<<endl;
}